import runpy
import subprocess
import sys

sys.path.insert(0, '@pkgdatadir@')

try:
    runpy.run_module('vectis')
except KeyboardInterrupt:
    raise SystemExit(130)
except subprocess.CalledProcessError as e:
    print(e, file=sys.stderr)
